#include<stdio.h>
 void main()
 {
     int a,b,result;
     float div;
     printf("enter two values");
     scanf("%d",&a);
     scanf("%d",&b);
     result=a+b;
     printf("sum=%d&%d\n",a,b,result);
     printf("difference=%d&%d=%d\n",a,b,a-b);
     printf("mltp=%d&%d=%d\n",a,b,a*b);
     div=a/b;
     printf("division=%d&%d=%f\n",a,b,div);
 }
